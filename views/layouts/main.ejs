    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.js" defer></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <style>
    @import url('https://fonts.googleapis.com/css2?family=Aladin&display=swap');
      html {
      align-items: center;
      justify-content: center;
      background-size: cover;
      overflow: hidden;
      box-sizing: border-box;
      background: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRrPKr2sL-YgDpAagTYdLVgABLmcE-ASdCzaA&usqp=CAU');
      }
      body, div, form, input, select, textarea, label { 
      outline: none;
      font-family: Aladin, cursive;
      font-size: 14px;
      opacity: 97%;
      color: #000000;
      line-height: 22px;
      }
      h1 {
      position: absolute;
      margin: 0;
      font-size: 40px;
      color: #fff;
      z-index: 2;
      line-height: 83px;
      }
      h2 {
        font-size: 20px;
        color: #494a4a;
        font-family: Aladin, cursive;
      }
      .testbox {
      display: flex;
      min-height: 100vh;
      box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
      justify-content: center;
      align-items: center;
      }
      .body {
      width: 100%;
      padding: 25px;
      border-radius: 6px;
      box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
      background-color: #ffffffab;
      box-shadow: 0 0 8px  #cc7a00; 
      }
      .banner {
      box-sizing: flex;
      position: relative;
      height: 150px;
      width: 100%;
      box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
      background-image: url("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRe6Snd3TBczxSyEl7Rfo8xqBPZLexci02k6w&usqp=CAU");
      border: 3px none #515151;
      border-top-right-radius: 10px 10px;
      border-top-left-radius: 10px 10px;
      background-color: #ffffff81;
      background-size: cover;
      display: flex;
      justify-content: center;
      align-items: center;
      text-align: center;
      }
      .banner::after {
      content: "";
      background-color: rgba(0, 0, 0, 0.2);
      box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
      position: absolute;
      border-top-right-radius: 10px 10px;
      border-top-left-radius: 10px 10px;
      width: 100%;
      height: 100%;
      }
      input, select, textarea {
      margin-bottom: 10px;
      border: 5px none #ccc;
      border-radius: 10px;
      box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
      }
      input {
      box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
      width: calc(100% - 10px);
      text-align: center;
      font-family: Aladin, cursive;
      height: 100px;
      padding: 5px;
      margin-top: 15px;
      }
      .item:hover p, .item:hover i, .question:hover p, .question label:hover, input:hover::placeholder {
      color: #9b0000;
      }
      .item input:hover, .item select:hover, .item textarea:hover {
      border: 1px none transparent;
      box-shadow: 0 0 3px 0 #890000;
      color: #8b0000;
      }
      .item:nth-child(2) {
      position: relative;
      margin: 10px 0;
      margin-bottom: 10px;
      box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
      border-bottom-right-radius: 10px 10px;
      border-bottom-left-radius: 10px 10px;
      }
      .btn-block {
      box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px, rgba(0, 0, 0, 0.3) 0px 7px 13px -3px, rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
      border-radius: 10px;
      margin-top: 10px;
      text-align: center;
      align-content: center;
      }
      button {
      width: 150px;
      padding: 10px;
      border: 5px ridge #d00000;
      font-family: Aladin, cursive;
      border-radius: 5px; 
      background: #7f0000;
      font-size: 16px;
      color: #fff;
      cursor: pointer;
      margin: 5px;
      box-shadow: 0 1px 6px rgba(0,0,0,0.16), 0 1px 6px rgba(0,0,0,0.23);
      }
      button:hover {
      box-shadow: 0 1px 6px rgba(0,0,0,0.16), 0 1px 6px rgba(0,0,0,0.23);
      background: #ad0000;
      outline: transparent;
      }
      @media (min-width: 568px) {
      .name-item, .city-item {
      text-align: center;
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;
      }
      .name-item input, .name-item div {
      text-align: center;
      width: calc(50% - 20px);
      }
      .name-item div input {
      text-align: center;
      width:97%;
      height: 98%;
      }
      .name-item div label {
      display:block;
      text-align: center;
      padding-bottom:5px;
      }
      }
    </style>
  </head>
  <body>
    <%- body %>
  </body>
  <script type="text/javascript" src="/config.js"></script>
</html>
